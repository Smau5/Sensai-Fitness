<div class="container">
    <h1>Fitness resource equipment List( <%= @fitness_resource.name %>)</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            <% @fitness_resource.equipments.distinct.each do |equipment| %>
            
                <tr>
                    <td><%= equipment.name %></td>
                </tr>
            <% end %>
        </tbody>
    </table>
    <%= link_to "Back", fitness_resources_path %>
</div>