<div class="container">
    <h1> Register!</h1>
    <%= form_for @company_lead do |c| %>
        <div class="form-group">
            <%= c.label :name, "Name" %>
            <%= c.text_field :name, class: "form-control"  %>
        </div>
        <div class="form-group">
            <%= c.label :lastName, "Last Name" %>
            <%= c.text_field :lastName, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= c.label :email, "Email" %>
            <%= c.text_field :email, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= c.label :cellphone, "CellPhone" %>
            <%= c.text_field :cellphone, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= c.label :company_lead_source_id, 'Source'%>
            <%= collection_select(:company_lead, :company_lead_source_id, CompanyLeadSource.all, :id, :description, {}, {class: "form-control"} ) %>
        </div>
        <%= c.submit class: "btn btn-xs btn-primary"%>
    <% end %>
</div>