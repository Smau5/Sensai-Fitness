<h1> Create Company Lead </h1>

<%= form_for @company_lead do |c| %>
    <%= c.label :name %>
    <%= c.text_field :name %>
    <%= c.label :lastName %>
    <%= c.text_field :lastName %>
    <%= c.label :email %>
    <%= c.text_field :email %>
    <%= c.label :cellphone %>
    <%= c.text_field :cellphone %>
    <%= collection_select(:company_lead, :company_lead_source_id, CompanyLeadSource.all, :id, :description, :prompt => true) %>
    <%= c.submit %>
<% end %>